{
  "name": "@tslstudio/engine",
  "version": "0.0.1",
  "description": "Renderer orchestration utilities for TSLStudio",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js"
    },
    "./capabilities": {
      "types": "./dist/capabilities/index.d.ts",
      "import": "./dist/capabilities/index.js"
    },
    "./framegraph": {
      "types": "./dist/framegraph/index.d.ts",
      "import": "./dist/framegraph/index.js"
    },
    "./loaders": {
      "types": "./dist/loaders/index.d.ts",
      "import": "./dist/loaders/index.js"
    },
    "./demo": {
      "types": "./dist/demo/index.d.ts",
      "import": "./dist/demo/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -w -p tsconfig.build.json",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest run"
  },
  "dependencies": {
    "@tslstudio/tsl-kit": "0.0.1",
    "@react-three/fiber": "8.15.16",
    "leva": "0.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.23.1",
    "three": "0.181.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "typescript": "5.4.5"
  }
}
